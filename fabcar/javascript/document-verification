Model Organization {
    id: string
    name: string
    type: string
}

Model User {
    id: string
    name: string
    email: string
    password: string
    organizations: Organization[]
}

Model Document {
    id: string
    name: string
    url: string
    issueDate: date
    expiryDate: date
    issuer: User
    hash: string,
    issuingOrg: Organization
}

(POST)
/api/register
    - email: string
    - password: string
    - Name: string

    Response:
        {
            status: 'successful' | 'failure'
            description: string
        }



(POST)
/api/login
    - email: string
    - password: string

    Cookies:
        - token: string
        - expireDate: date

    Response:
        {
            status: 'successful' | 'failure'
            description: string
        }



(GET)
/api/logout
    Cookies:
        - token: string
        - expireDate: date

    Response:
        {
            status: 'successful' | 'failure'
            description: string
        }


(POST)
/api/document
    - document: Document

    Response:
        {
            id, status, description
        }

(UPDATE)
/api/document/{id}
    - document: Document

    Response:
        {
            id, status, description
        }

(DELETE)
/api/document/{id}
    Response:
        {
            status, description
        }

(GET)
/api/document/{id}
    Response:
        {
            status, description, data: Document
        }

(GET)
/api/document
    Response:
        {
            status, description, data: Document[]
        }